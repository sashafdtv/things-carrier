{
  "manifest_version": 2,
  "name": "Things Carrier",
  "version": "1",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "description": "Расширения для переноса задач из platrum в things 3.",
  "browser_action": {
    "default_icon": "img/icon16.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "activeTab"
  ]
}